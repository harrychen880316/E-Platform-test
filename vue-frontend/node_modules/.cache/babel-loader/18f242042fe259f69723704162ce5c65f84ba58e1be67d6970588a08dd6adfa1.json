ton class=\"btn btn-success\" @click=\"newTutorial\">Add</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TutorialDataService from \"../services/TutorialDataService\";\r\n\r\nexport default {\r\n  name: \"add-tutorial\",\r\n  data() {\r\n    return {\r\n      tutorial: {\r\n        id: null,\r\n        title: \"\",\r\n        description: \"\",\r\n        published: false\r\n      },\r\n      submitted: false\r\n    };\r\n  },\r\n  methods: {\r\n    saveTutorial() {\r\n      var data = {\r\n        title: this.tutorial.title,\r\n        description: this.tutorial.description\r\n      };\r\n\r\n      TutorialDataService.create(data)\r\n          .then(response => {\r\n            this.tutorial.id = response.data.id;\r\n            console.log(response.data);\r\n            this.submitted = true;\r\n          })\r\n          .catch(e => {\r\n            console.log(e);\r\n          });\r\n    },\r\n\r\n    newTutorial() {\r\n      this.submitted = false;\r\n      this.tutorial = {};\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.submit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"mappings":"AAqCA,OAAAA,mBAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,EAAA;QACAC,KAAA;QACAC,WAAA;QACAC,SAAA;MACA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,aAAA;MACA,IAAAR,IAAA;QACAG,KAAA,OAAAF,QAAA,CAAAE,KAAA;QACAC,WAAA,OAAAH,QAAA,CAAAG;MACA;MAEAN,mBAAA,CAAAW,MAAA,CAAAT,IAAA,EACAU,IAAA,CAAAC,QAAA;QACA,KAAAV,QAAA,CAAAC,EAAA,GAAAS,QAAA,CAAAX,IAAA,CAAAE,EAAA;QACAU,OAAA,CAAAC,GAAA,CAAAF,QAAA,CAAAX,IAAA;QACA,KAAAM,SAAA;MACA,GACAQ,KAAA,CAAAC,CAAA;QACAH,OAAA,CAAAC,GAAA,CAAAE,CAAA;MACA;IACA;IAEAC,YAAA;MACA,KAAAV,SAAA;MACA,KAAAL,QAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}